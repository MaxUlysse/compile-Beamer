/*
vim: syntax=groovy
-*- mode: groovy;-*-
 * -------------------------------------------------
 * Nextflow config file
 * -------------------------------------------------
 * Default config options for all environments.
 */

manifest {
    homePage = 'https://github.com/MaxUlysse/compile-beamer'
    description = 'Compile Beamer file'
}

params {
	style_BTB = "beamer-templates/beamerthemeBTB.sty"
	style_KI  = "beamer-templates/beamerthemeKI.sty"
	style_SLL = "beamer-templates/beamerthemeSciLifeLab.sty"
	logo_BTB  = "beamer-templates/Barntumorbanken.pdf"
	logo_KI   = "beamer-templates/KI.pdf"
	logo_SLL  = "beamer-templates/SciLifeLab.pdf"
}